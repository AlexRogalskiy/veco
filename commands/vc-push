
usage() {
    echo "usage: vc push [ other options ]"
}


cmd_default() {
    vc_exec=$(get_vc_type)
    vc_args=""
    case $vc_exec in
        "hg")
            vc_args="push --new-branch"
            ;;
        "git")
            vc_args="push"
            ;;
        * )
            vc_args="push"
            ;;
    esac
    $vc_exec $vc_args "$@"
}


cmd_help() {
    usage
    exit 0
}

# /* vim: set filetype=sh : */
