
usage() {
    echo "usage: vc switch <branch-name>"
}


cmd_default() {
    vc_exec=$(get_vc_type)
    vc_args=""
    case $vc_exec in
        "svn")
            vc_exec="switch"
            ;;
        "hg" )
            vc_args="up"
            ;;
        "git")
            vc_args="checkout"
            ;;
    esac
    $vc_exec $vc_args "$@"
}


cmd_help() {
    usage
    exit 0
}

# /* vim: set filetype=sh : */
